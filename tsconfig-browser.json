{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "es5",
    "sourceMap": false,
    "moduleResolution": "Node",
    "outDir": "browser",
    "lib": ["es5", "es6", "es7", "esnext", "dom"]
  },
  "files": [
    "src/browser.ts",
    "src/actions.ts",
    "src/sequences.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
